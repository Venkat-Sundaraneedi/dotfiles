{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lsp/?.lua",
    "lua/?/init.lua"
  ],
  "diagnostics.globals": [
    "vim",
    "denols_handler",
    "Snacks",
    "nvim_lsp"
  ],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME"
  ],
  "diagnostics.disable": [
    "vim",
    "undefined-doc-name",
    "inject-field"
  ]
}
